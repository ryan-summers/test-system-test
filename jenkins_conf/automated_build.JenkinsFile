pipeline {

    agent any

    stages {
        stage('Source') {
            git 'https://github.com/ryan-summers/source-repo-test.git'
            make
            archiveArtifacts artifacts: 'build/*'
        }

        stage ('Tests') {
            git 'https://github.com/ryan-summers/test-system-test.git'
            make
            archiveArtifacts artifacts: 'build/**/*'
        }
    }
}
